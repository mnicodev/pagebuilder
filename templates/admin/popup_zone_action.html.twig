<ul>

	<li><a  id="style-zone">Style de la zone</a></li>
    <li><a href="#" id="delete-zone">Supprimer la zone</a></li>
</ul>
<script>
    $(document).ready(function () {
		$("#style-zone").click(function() {
			open_popup_style("{{ zone }}");
		})

        $("#delete-zone").click(function () {
			if(window.confirm("Suppimer la zone {{ zone }} ?"))
                parent.document.getElementById("{{ zone }}").remove();
        })
    })

</script>